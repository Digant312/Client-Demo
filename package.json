{
  "name": "argomi",
  "version": "1.0.0",
  "description": "Argomi Web Platform",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --progress",
    "start:staging":
      "webpack-dev-server --config webpack.staging.config.js --progress",
    "build": "webpack -d --progress",
    "build:production": "webpack -p --config webpack.prod.config.js --progress",
    "build:staging": "webpack -p --config webpack.staging.config.js --progress",
    "build:analyze": "ANALYZER_MODE=static npm run build:production",
    "test": "jest",
    "test:watch": "jest --watch",
    "messages": "node combineMessages.js"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$":
        "<rootDir>/__mocks__/fileMock.ts",
      "\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.ts"
    },
    "setupFiles": ["jest-localstorage-mock"],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "test-tsconfig.json",
        "useBabelrc": true
      }
    },
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js",
      ".(js|jsx)": "babel-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": ["ts", "tsx", "jsx", "js", "json"],
    "testPathIgnorePatterns": ["<rootDir>/node_modules/", "<rootDir>/dist/"],
    "modulePaths": ["<rootDir>/src/"]
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/aws-iot-device-sdk": "^2.1.0",
    "@types/classnames": "^2.2.3",
    "@types/enzyme": "^2.8.6",
    "@types/glob": "^5.0.32",
    "@types/jest": "^20.0.8",
    "@types/lodash": "^4.14.74",
    "@types/mkdirp": "^0.5.1",
    "@types/moment-business": "^0.1.4",
    "@types/node": "^8.0.25",
    "@types/numeral": "0.0.22",
    "@types/react": "^15.6.2",
    "@types/react-datepicker": "^0.55.0",
    "@types/react-dom": "^15.5.4",
    "@types/react-intl": "^2.3.1",
    "@types/react-intl-redux": "^0.1.3",
    "@types/react-loadable": "^4.0.4",
    "@types/react-redux": "^4.4.47",
    "@types/react-router-dom": "^4.0.7",
    "@types/react-transition-group": "^2.0.6",
    "@types/redux-form": "^6.6.10",
    "@types/redux-logger": "^3.0.1",
    "@types/uuid": "^3.4.1",
    "awesome-typescript-loader": "^3.2.3",
    "babel-core": "^6.26.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-dynamic-import-node": "^1.0.2",
    "babel-plugin-react-intl": "^2.3.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.5",
    "enzyme": "^2.9.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "fs": "0.0.1-security",
    "glob": "^7.1.2",
    "html-webpack-plugin": "^2.30.1",
    "inline-manifest-webpack-plugin": "^3.0.1",
    "jest": "^20.0.4",
    "jest-localstorage-mock": "^1.2.0",
    "mkdirp": "^0.5.1",
    "mock-local-storage": "^1.0.4",
    "node-sass": "^4.5.3",
    "path": "^0.12.7",
    "react-redux": "^5.0.6",
    "react-test-renderer": "^15.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "ts-jest": "^20.0.10",
    "typescript": "2.5.3",
    "webpack": "^3.5.5",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.7.1"
  },
  "dependencies": {
    "@amaas/amaas-core-sdk-js": "1.1.0",
    "@types/moment-timezone": "^0.5.0",
    "amazon-cognito-identity-js": "^1.19.0",
    "aws-iot-device-sdk": "^2.1.0",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.5",
    "lodash": "^4.17.4",
    "moment": "~2.18.1",
    "moment-business": "^3.0.1",
    "moment-timezone": "~0.5.13",
    "numeral": "^2.0.6",
    "rc-time-picker": "^2.4.1",
